depends: [
"lodash__isnative" {>="2.4.1" & <"2.5.0"}

"lodash_isobject" {>="2.4.1" & <"2.5.0"}

"lodash__shimkeys" {>="2.4.1" & <"2.5.0"}

]
install: ["sh" "-c" "ln -s %{PKG:lib}% %{lib}%/lodash.keys"]

remove: [
  [ "sh" "-c" "rm %{lib}%/lodash.keys" ] 
]

