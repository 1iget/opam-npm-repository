depends: [
"lodash__baseclone" {>="2.1.0" & <"2.2.0"}

"lodash__basecreatecallback" {>="2.1.0" & <"2.2.0"}

]
install: ["sh" "-c" "ln -s %{lib}%/lodash_clone %{lib}%/lodash.clone"]

remove: [
  [ "sh" "-c" "rm %{lib}%/lodash.clone" ] 
]

